{
    "name": "resultakak/api",
    "authors": [
        {
            "name": "Resul Takak",
            "email": "resultakak@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-psr": "*",
        "ext-phalcon": "^4.0.0",
        "monolog/monolog": "^2.3",
        "robmorgan/phinx": "^0.12.7",
        "symfony/service-contracts": "2.2.0"
    },
    "autoload": {
        "psr-4": {
            "Api\\": "src/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^9.5",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^0.12.93",
        "squizlabs/php_codesniffer": "3.*",
        "vimeo/psalm": "^4.8",
        "orklah/psalm-insane-comparison": "^1.0"
